{
    "name": "drewlabs/auth-jwt-guard",
    "description": "JWT Auth Guard composer for drewlabs/auth library",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "auth",
        "jwt"
    ],
    "autoload": {
        "psr-4": {
            "Drewlabs\\Auth\\JwtGuard\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Drewlabs\\Auth\\JwtGuard\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "azandrew-sidoine",
            "email": "azandrewdevelopper@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2|^8.0",
        "drewlabs/auth-jwt": "^0.2.0|^0.3.0",
        "drewlabs/http-cookie": "^0.2.0",
        "illuminate/console": "^9.0|^10.0|^11.0",
        "phpseclib/phpseclib": "^3.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "illuminate/auth": "^9.0|^10.0|^11.0",
        "illuminate/http": "^9.0|^10.0|^11.0",
        "phpunit/phpunit": ">=6.0",
        "vlucas/phpdotenv": ">=3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Drewlabs\\Auth\\JwtGuard\\ServiceProvider"
            ]
        }
    }
}
